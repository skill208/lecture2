<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kcj.myapplication.MainActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="190dp"
            android:layout_height="253dp"
            android:layout_alignLeft="@+id/imageButton2"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/imageButton2"
            android:layout_column="1"
            android:layout_row="1"
            app:srcCompat="@drawable/an4"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="190dp"
            android:layout_height="253dp"
            android:layout_alignEnd="@+id/imageButton"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignRight="@+id/imageButton"
            android:layout_alignTop="@+id/imageButton4"
            android:layout_column="0"
            android:layout_row="1"
            app:srcCompat="@drawable/an3"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="-1dp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="190dp"
            android:layout_height="253dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            app:srcCompat="@drawable/an2"
            tools:layout_editor_absoluteX="198dp"
            tools:layout_editor_absoluteY="8dp" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="190dp"
            android:layout_height="253dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            app:srcCompat="@drawable/an1"
            tools:layout_editor_absoluteX="-4dp"
            tools:layout_editor_absoluteY="-1dp" />

    </RelativeLayout>

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="130dp"
        android:layout_height="130dp"
        app:srcCompat="@drawable/an6"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="195dp" />

</android.support.constraint.ConstraintLayout>
